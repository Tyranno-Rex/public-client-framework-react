{
  "name": "public-client-framework-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "proto:generate": "npm run proto:clean && npm run proto:gen",
    "proto:clean": "rm -rf src/packages/grpc/generated",
    "proto:gen": "protoc -I=proto proto/*.proto --js_out=import_style=commonjs,binary:src/packages/grpc/generated --grpc-web_out=import_style=typescript,mode=grpcwebtext:src/packages/grpc/generated"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "axios": "^1.13.2",
    "framer-motion": "^12.27.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "google-protobuf": "^4.0.1",
    "grpc-web": "^2.0.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "protoc-gen-grpc-web": "^1.5.0",
    "tailwindcss": "^4.1.18",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
  }
}
